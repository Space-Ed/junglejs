{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": true,
        "noImplicitAny": false,
        "noImplicitUseStrict": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outFile": "../dist/jungle.js"
    },
    "exclude": [
        "node_modules",
        "typings/browser",
        "typings/browser.d.ts"
    ],
    "compileOnSave": false,
    "buildOnSave": false,
    "atom": {
        "rewriteTsconfig": true
    },
    "filesGlob": [
        "typings/index.d.ts",
        "./**/*.ts"
    ],
    "files": [
        "./construction/base/construct.ts",
        "./construction/base/domain.ts",
        "./construction/cell.ts",
        "./construction/complex/composite.ts",
        "./construction/complex/state.ts",
        "./construction/hooks/call.ts",
        "./construction/hooks/hook.ts",
        "./construction/hooks/visor.ts",
        "./interoperability/base/crux.ts",
        "./interoperability/base/interfaces.ts",
        "./interoperability/base/medium.ts",
        "./interoperability/base/membrane.ts",
        "./interoperability/base/meshHost.ts",
        "./interoperability/base/visor.ts",
        "./interoperability/call/crux.ts",
        "./interoperability/call/distributive.ts",
        "./interoperability/call/injective.ts",
        "./interoperability/call/pushAlias.ts",
        "./interoperability/designation/designable.ts",
        "./jungle.ts",
        "./test/mockConstruct.ts",
        "./util/blender.ts",
        "./util/checks.ts",
        "./util/debug.ts",
        "./util/junction.ts",
        "./util/math.ts",
        "./util/misc.ts",
        "./util/transforms.ts",
        "./util/typesplit.ts"
    ]
}
