{
    "compilerOptions": {
        "target": "es5",
        "module": "amd",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": true,
        "noImplicitAny": false,
        "noImplicitUseStrict": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outFile": "../dist/jungle.js"
    },
    "exclude": [
        "node_modules",
        "typings/browser",
        "typings/browser.d.ts"
    ],
    "compileOnSave": false,
    "buildOnSave": false,
    "atom": {
        "rewriteTsconfig": true
    },
    "filesGlob": [
        "typings/index.d.ts",
        "./**/*.ts"
    ],
    "files": [
        "./aliases.ts",
        "./base/actions.ts",
        "./base/context.ts",
        "./base/core.ts",
        "./base/form.ts",
        "./base/io.ts",
        "./base/ports.ts",
        "./base/shell.ts",
        "./inventory/io.ts",
        "./inventory/resolver.ts",
        "./inventory/select.ts",
        "./inventory/standards.ts",
        "./link/core.ts",
        "./link/form.ts",
        "./link/io.ts",
        "./main.ts",
        "./nova/construct/base/construct.ts",
        "./nova/construct/base/domain.ts",
        "./nova/construct/cell.ts",
        "./nova/construct/complex/composite.ts",
        "./nova/construct/complex/state.ts",
        "./nova/construct/hooks/call.ts",
        "./nova/construct/hooks/hook.ts",
        "./nova/construct/hooks/visor.ts",
        "./nova/interoperability/base/crux.ts",
        "./nova/interoperability/base/designable.ts",
        "./nova/interoperability/base/interfaces.ts",
        "./nova/interoperability/base/medium.ts",
        "./nova/interoperability/base/membrane.ts",
        "./nova/interoperability/base/meshHost.ts",
        "./nova/interoperability/base/visor.ts",
        "./nova/interoperability/call/crux.ts",
        "./nova/interoperability/call/distributive.ts",
        "./nova/interoperability/call/injective.ts",
        "./nova/interoperability/call/pushAlias.ts",
        "./nova/test/mockConstruct.ts",
        "./reconstruction.ts",
        "./resolve/core.ts",
        "./resolve/form.ts",
        "./resolve/io.ts",
        "./resolve/ports.ts",
        "./resolve/shell.ts",
        "./terminal.ts",
        "./util/blender.ts",
        "./util/checks.ts",
        "./util/debug.ts",
        "./util/junction.ts",
        "./util/math.ts",
        "./util/misc.ts",
        "./util/transforms.ts",
        "./util/typesplit.ts"
    ]
}
